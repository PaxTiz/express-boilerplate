{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src src/app.ts",
    "build": "tsc --allowSyntheticDefaultImports",
    "start": "NODE_ENV=production node ./dist/app.js",
    "prisma:migrate": "npx prisma migrate dev && npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "prisma:pull": "npx prisma db pull",
    "prisma:generate": "npx prisma generate",
    "test:e2e": "./node_modules/ts-mocha/bin/ts-mocha",
    "test:e2e:all": "./node_modules/ts-mocha/bin/ts-mocha \"test/e2e/**/*.ts\"",
    "lint": "./node_modules/eslint/bin/eslint.js '*/**/*.{js,ts}' --quiet",
    "lint:fix": "./node_modules/eslint/bin/eslint.js '*/**/*.{js,ts}' --quiet --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@sentry/node": "^6.17.9",
    "@sentry/tracing": "^6.17.7",
    "bcrypt": "^5.0.1",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.12.1",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "prisma": "^3.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "axios": "^0.26.0",
    "chai": "^4.3.4",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.12",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.3.5"
  }
}
